From 0980168f7689a7f3b893c154bc1d3adfa7f72067 Mon Sep 17 00:00:00 2001
From: S-trace <S-trace@list.ru>
Date: Fri, 2 Sep 2016 02:55:39 +0300
Subject: [PATCH] [BOOT] Keymaster: Enable keymaster service

This fixes Settings crashes and reboot on apps uninstall
---
 ramdisk/init.rc | 5 +----
 1 file changed, 1 insertion(+), 4 deletions(-)

diff --git a/ramdisk/init.rc b/ramdisk/init.rc
index f56e0f3..634c923 100755
--- a/ramdisk/init.rc
+++ b/ramdisk/init.rc
@@ -592,12 +592,9 @@ service mtpd /system/bin/mtpd
     oneshot
 
 service keystore /system/bin/keystore /data/misc/keystore
+    class main
     user keystore
     group keystore drmrpc system
-    disabled
-
-on property:sys.keymaster.loaded=true
-    start keystore
 
 service dumpstate /system/bin/dumpstate -s
     class main
-- 
2.7.4

